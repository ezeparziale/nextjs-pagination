generator client {
  provider        = "prisma-client"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "postgresql"
}

model Pokemon {
  id             Int    @id @default(autoincrement())
  name           String @unique
  height         Int
  weight         Int
  hp             Int
  attack         Int
  defense        Int
  specialAttack  Int    @map("special_attack")
  specialDefense Int    @map("special_defense")
  speed          Int

  @@map("pokemons")
}
